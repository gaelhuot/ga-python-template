# Environment variables example
# Copy this file to .env and modify the values as needed

# Application settings
PROJECT_NAME=GA Python Template
PROJECT_DESCRIPTION=Simple python template
VERSION=0.1.0
API_V1_STR=/api/v1

# CORS settings (comma-separated list)
BACKEND_CORS_ORIGINS=*  # In production, specify actual domains: https://yourdomain.com,https://app.yourdomain.com

# Server settings
HOST=0.0.0.0
PORT=8000
DEBUG=false

# Environment
ENVIRONMENT=development  # Set to "production" for prod deployment

# API Documentation (optional)
CONTACT_NAME=Your Name
CONTACT_EMAIL=your.email@example.com
CONTACT_URL=https://yourwebsite.com
LICENSE_NAME=MIT
LICENSE_URL=https://opensource.org/licenses/MIT
TERMS_OF_SERVICE=https://yourwebsite.com/terms
